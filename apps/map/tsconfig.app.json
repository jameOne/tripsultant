{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "../../dist/out-tsc",
    "types": [],
    "baseUrl": "./",
    "paths": {
      "~app/*": [
        "src/app/*"
      ],
      "~assets/*": [
        "src/assets/*"
      ],
      "~environments/*": [
        "src/environments/*"
      ],
      "~guards/*": [
        "src/guards/*"
      ],
      "~interceptors/*": [
        "src/interceptors/*"
      ],
      "~leaflet/*": [
        "src/leaflet/*"
      ],
      "~models/*": [
        "src/models/*"
      ],
      "~services/*": [
        "src/services/*"
      ],
      "~store/*": [
        "src/store/*"
      ],
      "@tripsultant/ngrx-analytics": ["../../libs/ngrx-analytics/src/index.ts"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"]
}
